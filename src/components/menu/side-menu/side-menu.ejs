<script>

    //side menu fixed desktop
    let sideMenu = document.querySelector('.side-menu');

    document.addEventListener('scroll', () => {
        if (pageYOffset > 300) {
            sideMenu.classList.add('fixed');
        } else {
            sideMenu.classList.remove('fixed');
        }
    });

    //side menu mobile
    let sideMenuMobile = document.querySelector('.side-menu-mobile');
    let locationSelectMobileOpenBtn = sideMenuMobile.querySelector('[data-search-modal-btn]');
    let locationSelectMobileCloseBtn = sideMenuMobile.querySelector('[data-search-modal-close]');
    let sideMenuMobileOpenPtn = document.querySelector('.header-side-menu-mobile__open-btn');
    let sideMenuMobileClosePtn = sideMenuMobile.querySelector('.side-menu-mobile__close-btn');

    locationSelectMobileOpenBtn.addEventListener('click', function (ev) {
        sideMenuMobile.style.overflow = 'hidden';
    });

    locationSelectMobileCloseBtn.addEventListener('click', function (ev) {
        sideMenuMobile.style.overflow = 'auto';
    });

    sideMenuMobileOpenPtn.addEventListener('click', () => {
        if (sideMenuMobile) {
            sideMenuMobile.classList.add('open');
            document.body.style.overflow = "hidden";
        }
    });

    sideMenuMobileClosePtn.addEventListener('click', () => {
        if (sideMenuMobile) {
            sideMenuMobile.classList.remove('open');
            document.body.style.overflow = "visible";
        }
    });

    document.addEventListener('click', ev => {
        if (!ev.target.closest('.side-menu-mobile') && !ev.target.closest('.header-side-menu-mobile__open-btn')) {
            sideMenuMobile.classList.remove('open');
            document.body.style.overflow = "visible"
            sideMenuMobile.style.overflow = 'auto';
        }
    });

</script>